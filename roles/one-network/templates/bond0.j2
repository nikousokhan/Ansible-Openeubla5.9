#jinja2: trim_blocks:False
# Generated by ansible in {{ date_time }}
{% for interface in interfaces_Ten_G %}

auto {{ interface }}
iface {{ interface }} inet manual
    mtu 1500
    up ifconfig {{ interface }} up
    bond-master bond0

{%- endfor %}

auto bond0
iface bond0 inet manual
    bond-slaves {% for interface in interfaces_Ten_G %} {{ interface }} {%- endfor %}
    bond-mode 4
    bond-miimon 100
    bond-downdelay 200
    bond-updelay 200
    mtu 1500
    bond-xmit-hash-policy layer3+4
    txqueuelen 10000

